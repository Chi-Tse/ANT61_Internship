    "import numpy as np\n",
    "def swap_rows(mat: np.ndarray):\n",
    "    if mat.shape[0]%2 == 0:\n",
    "        for i in range(0, mat.shape[0], 2):\n",
    "            mat[[i,i+1]] = mat[[i+1,i]]"
   ]
